{
  "name": "macos-a11y-tree",
  "version": "1.0.0",
  "description": "Extract macOS accessibility tree as JSON for specified windows",
  "main": "src/lib/index.ts",
  "module": "src/lib/index.ts",
  "type": "module",
  "exports": {
    ".": "./src/lib/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --target=bun --outdir=dist",
    "list": "bun run src/index.ts --list"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0"
  },
  "dependencies": {
    "node-mac-permissions": "^2.3.0",
    "sharp": "^0.34.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}